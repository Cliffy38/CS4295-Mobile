<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/Setting"
    android:background="@drawable/gradient"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:id="@+id/WorkSection"
        android:layout_width="@dimen/abc_panel_menu_list_width"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:lines="1"
        android:paddingBottom="10dp"
        android:paddingLeft="10dp"
        android:paddingRight="0dp"
        android:paddingTop="15dp"
        android:text="WorkOut"
        android:textColor="#ffffff"
        android:textSize="25sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />
<LinearLayout
    android:id="@+id/three_of_setting"
    android:layout_below="@+id/two_of_setting"
    android:layout_width="fill_parent"
    android:layout_height="80dp"
    android:orientation="vertical"
    >

    <TextView
        android:id="@+id/breakTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:lines="1"
        android:paddingBottom="0dp"
        android:paddingLeft="55dp"
        android:paddingRight="0dp"
        android:paddingTop="13dp"
        android:text="Break Time"
        android:textColor="#ffffff"
        android:textSize="25sp"
         />

    <TextView
        android:id="@+id/Break"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60dp"
        android:gravity="center"
        android:lines="1"

        android:text="0"
        android:textColor="#000000"
        android:textSize="17sp"
        android:layout_marginBottom="10dp"/>


</LinearLayout>

    <LinearLayout
        android:id="@+id/two_of_setting"
        android:layout_below="@+id/first_of_setting"
        android:layout_width="fill_parent"
        android:layout_height="80dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/workoutTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:lines="1"
            android:paddingBottom="0dp"
            android:paddingLeft="55dp"
            android:paddingRight="0dp"
            android:paddingTop="7dp"
            android:text="Workout Time"
            android:textColor="#ffffff"
            android:textSize="25sp"
            />

        <TextView
        android:id="@+id/Workout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60dp"
        android:gravity="center"
        android:lines="1"


        android:text="0"
        android:textColor="#000000"
        android:textSize="17sp" />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/first_of_setting"
        android:layout_width="fill_parent"
        android:layout_height="80dp"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true"
        android:orientation="vertical">



        <TextView
            android:id="@+id/repetitionTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:lines="1"
            android:paddingBottom="0dp"
            android:paddingLeft="55dp"
            android:paddingRight="0dp"
            android:paddingTop="15dp"
            android:text="Repeat Circuits"
            android:textColor="#ffffff"
            android:textSize="25sp"
            android:layout_alignBottom="@+id/textView"
            android:layout_alignParentStart="true" />

        <TextView
            android:id="@+id/Repetition"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="60dp"
            android:gravity="center"
            android:lines="1"
            android:layout_marginBottom="10dp"

            android:text="0"
            android:textColor="#000000"
            android:textSize="17sp" />

            />
    </LinearLayout>
    <View
        android:layout_width="fill_parent"
        android:layout_height="2px"
        android:background="#5b5c5c"
        android:id="@+id/textView"
        android:layout_below="@+id/WorkSection"
        android:layout_alignParentStart="true" />
    <View
        android:layout_width="fill_parent"
        android:layout_height="2px"
        android:background="#5b5c5c"
        android:layout_below="@+id/first_of_setting"
        android:layout_alignParentStart="true"
       />

    <View
        android:layout_width="fill_parent"
        android:layout_height="2px"
        android:background="#5b5c5c"
        android:layout_below="@+id/two_of_setting"
        android:layout_alignParentStart="true"
         />

    <View
        android:layout_width="fill_parent"
        android:layout_height="2px"
        android:background="#5b5c5c"
        android:layout_below="@+id/three_of_setting"
        android:layout_alignParentStart="true" />

</RelativeLayout>